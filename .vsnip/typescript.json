{
  "consoleLog": {
    "prefix": "cl",
    "body": "console.log($1);",
    "description": "Console log statement"
  },
  "consoleError": {
    "prefix": "ce",
    "body": "console.error($1);",
    "description": "Console error statement"
  },
  "consoleWarn": {
    "prefix": "cw",
    "body": "console.warn($1);",
    "description": "Console warn statement"
  },
  "eslintDisableNextLine": {
    "prefix": "edl",
    "body": "// eslint-disable-next-line $1",
    "description": "Comment to disable eslint on the next line"
  }
}
